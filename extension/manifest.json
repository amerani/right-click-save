{
  "name": "right-click-save",
  "description": "Stores base64 encoded string for up to 10 svg elements on the page where it's executed",
  "version": "0.0.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "src/*",
        "node_modules/**/*"
      ],
      "matches": ["<all_urls>"],
      "use_dynamic_url": true
    }
  ]
}